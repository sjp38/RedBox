<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout android:layout_height="wrap_content"
        android:layout_width="match_parent" android:id="@+id/linearLayout1"
        android:orientation="horizontal">
        <TextView android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_height="wrap_content" android:id="@+id/number_textview"
            android:layout_width="wrap_content" android:text="TextView" android:layout_gravity="center" android:layout_weight="9"></TextView>
        <CheckBox android:layout_height="wrap_content" android:text="" android:id="@+id/number_enabled_checkBox" android:layout_width="wrap_content" android:layout_gravity="center"></CheckBox>

    </LinearLayout>
</LinearLayout>
